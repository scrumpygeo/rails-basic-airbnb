<!DOCTYPE html>
<html>
  <head>
    <title>RailsBasicAirbnb</title>
    <%= csrf_meta_tags %>
    <%= csp_meta_tag %>

    <%= stylesheet_link_tag 'application', media: 'all', 'data-turbolinks-track': 'reload' %>
    <%= javascript_pack_tag 'application', 'data-turbolinks-track': 'reload' %>
    <meta http-equiv="X-UA-Compatible" content="IE=edge">
    <meta name="viewport" content="width=device-width, initial-scale=1">

      <link href="https://fonts.googleapis.com/icon?family=Material+Icons" rel="stylesheet">
  </head>

  <body>
  <nav class="navbar navbar-light bg-light justify-content-between sticky-top">
    <a class="navbar-brand">Simple Airbnb</a>
    <form class="form-inline">
      <button class="btn btn-outline-success my-2 my-sm-0" type="submit">Add Flat</button>
    </form>
  </nav>






  <div class="container">
    <%= yield %>
    </div>
  </body>
</html>
